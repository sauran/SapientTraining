package package1;


enum Apple{
	Jonathan(10),GoldenDel(9),RedDel(8),Winesap(7),Cortland(6);
	
	private int price;
	Apple(int p){
		price = p;
		
	}
	int getPrice(){
		return price;
	}
	
}
