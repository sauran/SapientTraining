 */
package com.sapient.math;

import com.sapient.cui.client.Confirmed;
import com.sapient.cui.client.Contract;
import com.sapient.cui.client.Employee;
import com.sapient.cui.client.Intern;

/**
 * @author span42
 *
 */
public class HR {
	
	public Employee recruit(String empType) {
		if (empType.equals("I")) {
			return new Intern();
		} else if (empType.equals("C")) {
			return new Confirmed();
		}
		 else if (empType.equals("T")) {
				return new Contract();
		
	}
		return null;
}

}
